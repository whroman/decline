  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --progress --config tasks/dev.webpack.config.js",
    "build": "webpack --progress --config tasks/build.webpack.config.js",
    "test": "mocha --compilers js:babel-register $(find ./generator ./app ./tables -name '*.spec.js')",
    "test:watch": "npm test -- --watch",
    "tdd": "npm run test:watch",
    "lint": "npm run lint:client && npm run lint:specs",
    "lint:client": "esw --ignore-pattern *.spec.js --config ./tasks/.eslintrc.client.js ./tables ./src ./tasks ./app ./generator",
    "lint:specs": "esw --config ./tasks/.eslintrc.mocha.js './tables/**/*.spec.js' './src/**/*.spec.js' './generator/**/*.spec.js'",
    "lint:watch": "run-p lint:watch:client lint:watch:specs",
    "lint:watch:client": "npm run lint:client -- --watch",
    "lint:watch:specs": "npm run lint:specs -- --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "0.11.0",
    "foundation-sites": "6.2.1",
    "lodash": "4.12.0",
