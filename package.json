    "babel-polyfill": "6.8.0",
    "babel-preset-es2016": "6.0.11",
    "babel-preset-react": "6.5.0",
    "babel-register": "6.9.0",
    "chai": "3.4.1",
    "css-loader": "0.23.1",
    "eslint": "2.10.2",
    "eslint-plugin-babel": "3.2.0",
    "eslint-plugin-mocha": "2.2.0",
    "eslint-plugin-react": "5.1.1",
    "eslint-watch": "2.1.11",
    "extract-text-webpack-plugin": "1.0.1",
    "mocha": "2.3.4",
    "node-sass": "3.7.0",
    "npm-run-all": "2.1.1",
