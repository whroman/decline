    "react-addons-css-transition-group": "15.1.0",
    "react-dom": "15.1.0",
    "react-dropdown": "1.0.4",
    "react-ga": "2.1.0",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
    "react-router-redux": "4.0.4",
    "redux": "3.5.2",
    "redux-actions": "0.9.1",
    "redux-logger": "2.6.1"
  },
  "devDependencies": {
    "babel-eslint": "6.0.4",
    "babel-loader": "6.2.4",
